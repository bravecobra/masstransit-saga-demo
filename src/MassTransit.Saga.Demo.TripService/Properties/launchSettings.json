{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "MassTransit.Saga.Demo.TripService": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "demo/index.html",
      "environmentVariables": {
        "ASPNETCORE_BASEPATH": "/demo",
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:5001;http://localhost:5000"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/product",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}